<main id="admin">

    <header>
        <nav>
            <a href="/admin">list</a>
            <a href="/admin/add">add country</a>
        </nav>
    </header>

    <section>

        <h2>Countries List</h2>
        <table>
            <thead>
                <tr>
                    <th>id</th>
                    <th>label</th>
                    <th>picture</th>
                    <th>recipe</th>
                    <th>action</th>
                </tr>
            </thead>

            <tbody>
                <!-- boucle -->
                <% for( const data of datas ) { %>
                    <tr>
                        <td><%= data.id %></td>               
                        <td><%= data.label %></td>
                        <td><%= data.picture.country %></td>
                        <td><%= data.recipe %></td>
                        <td>
                            <button>edit</button>
                            <button>delete</button>
                        </td>
                    </tr>
                    <%}%>
            </tbody>
        </table>
    </section>


</main>